{
"manifest_version": 3,
"name": "YouTube Video Q&A",
"version": "1.0",
"description": "Ask questions about any YouTube video with a transcript.",
"permissions": [
"activeTab",
"scripting"
],
"host_permissions": [
"https://www.google.com/search?q=http://127.0.0.1:5000/*"
],
"action": {
"default_popup": "popup.html",
"default_icon": {
"48": "images2.png"
}
},
"icons": {
"48": "images2.png",
"128": "images2.png"
}
}